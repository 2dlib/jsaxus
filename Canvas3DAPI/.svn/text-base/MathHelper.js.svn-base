// Copying an object
function copyObj(copyObj)
{
	var obj = new Object();
	
	for (i in copyObj)
	{
		obj[i] = copyObj[i];
	}
    
	return obj;
}

function invSqrt(num)
{
	if (!isNaN(num))
	{
		// Why JS doesn't have this, I will never know
		return 1 / Math.sqrt(num);
	}
	
	return 0;
}